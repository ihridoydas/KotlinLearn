<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.jetpackcomposepractice">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--    For Permission Activity-->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:name=".BaseApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CustomSplashScreenTheme"
        tools:targetApi="31">
        <activity
            android:name=".permissionCheck.PermissionActivity"
            android:exported="false"
            android:label="@string/title_activity_permission"
            android:theme="@style/Theme.CustomSplashScreenTheme" />
        <activity
            android:name=".onboardingCompose.BoardingWithSplashScreen"
            android:exported="false"
            android:label="@string/title_activity_boarding_with_splash_screen"
            android:theme="@style/Theme.CustomSplashScreenTheme"></activity>
        <activity
            android:name=".paging3.Paging3Activity"
            android:exported="false"
            android:label="@string/title_activity_paging3"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".topBarWithNavGraph.TopBarNavGraphActivity"
            android:exported="false"
            android:label="@string/title_activity_top_bar_nav_graph"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".tabLayout.TabLayoutActivity"
            android:exported="false"
            android:label="@string/title_activity_tab_layout"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".todoMVVM.TodoMainActivity"
            android:exported="false"
            android:label="@string/title_activity_todo_main"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".learnTesting.TestingActivity"
            android:exported="false"
            android:label="@string/title_activity_testing"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".uiPractice.loginScreen.LoginActivity"
            android:exported="false"
            android:label="Login Activity"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".albumFeature.AlbumDetailsActivity"
            android:exported="false"
            android:label="@string/title_activity_album_details"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".navigationPassingDataNavHost.PassingDataNavHost"
            android:exported="false"
            android:label="@string/title_activity_passing_data_nav_host"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".screen.NavGraphActivity"
            android:exported="false"
            android:label="@string/title_activity_nav_graph"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".nestedNavigation.NestedNavigation"
            android:exported="false"
            android:label="@string/title_activity_nested_navigation"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".feature_note.NoteActivity"
            android:exported="false"
            android:label="Note Activity"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".retrofitAPI.RetrofitActivity"
            android:exported="false"
            android:label="Practice API Retrofit"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".LazyColumnActivity"
            android:exported="false"
            android:label="@string/title_activity_another"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".todo.ui.TodoActivity"
            android:exported="false"
            android:label="ToDoActivity"
            android:theme="@style/Theme.JetPackComposePractice" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CustomSplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".viewPager.OnBoardingActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetPackComposePractice">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->

        </activity>
    </application>

</manifest>